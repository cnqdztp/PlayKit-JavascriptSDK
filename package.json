{
  "name": "playkit-sdk",
  "version": "1.1.3-beta.4",
  "description": "PlayKit SDK for JavaScript",
  "main": "dist/playkit-sdk.cjs.js",
  "module": "dist/playkit-sdk.esm.js",
  "browser": "dist/playkit-sdk.umd.js",
  "types": "dist/playkit-sdk.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "cross-env PLAYKIT_BASE_URL=http://localhost:3000 rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "sdk",
    "playkit",
    "game",
    "chat",
    "chatgpt",
    "image-generation",
    "npc",
    "p5js",
    "game-engine",
    "llm",
    "openai"
  ],
  "author": "capsuleer",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/cnqdztp/DeveloperWorks-JavascriptSDK"
  },
  "bugs": {
    "url": "https://github.com/cnqdztp/DeveloperWorks-JavascriptSDK/issues"
  },
  "homepage": "https://github.com/cnqdztp/DeveloperWorks-JavascriptSDK#readme",
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
